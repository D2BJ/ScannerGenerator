$DIGIT [0-9]
$NON-ZERO [^0] IN $DIGIT
$CHAR [a-zA-Z]
$UPPER [^a-z] IN $CHAR
$LOWER [^A-Z] IN $CHAR

$IDENTIFIER $LOWER ($LOWER|$DIGIT)*
$INT ($DIGIT)+
$FLOAT ($DIGIT)+ \. ($DIGIT)+
$ASSIGN =
$PLUS \+
$FART PRINT\*
$MINUS -
$MULTIPLY \*
$PRINT PRINT
$ANNOY a($DIGIT)+b
$FARK ($DIGIT)($UPPER)($LOWER)
$ABC (a|b|c)TOAD
